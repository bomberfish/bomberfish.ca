{
	"name": "website",
	"private": true,
	"version": "8.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build:client": "tsc && vite build --ssrManifest .vite/ssr-manifest.json --outDir dist/static",
		"build:server": "tsc && vite build --ssr src/main-server.ts --outDir dist/server",
		"build": "npm run build:client && npm run build:server && node prerender.js",
		"format": "prettier --write src/"
	},
	"devDependencies": {
		"@literals/rollup-plugin-html-css-minifier": "^3.0.1",
		"@mdx-js/mdx": "^3.1.1",
		"@types/humanize-duration": "^3.27.4",
		"@types/node": "^24.10.1",
		"@vitejs/plugin-legacy": "^7.2.1",
		"@wooorm/starry-night": "^3.8.0",
		"cssnano": "^7.1.2",
		"esbuild": "^0.25.12",
		"estree-util-visit": "^2.0.0",
		"lightningcss": "^1.30.2",
		"postcss": "^8.5.6",
		"postcss-cli": "^11.0.1",
		"prettier": "next",
		"react": "^19.2.0",
		"rehype-starry-night": "^2.2.0",
		"terser": "^5.44.1",
		"typescript": "next",
		"vite": "npm:rolldown-vite@^7.2.7"
	},
	"dependencies": {
		"autoprefixer": "^10.4.22",
		"dreamland": "^0.1.2",
		"humanize-duration": "^3.33.1",
		"postcss-preset-env": "^10.4.0"
	}
